FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    object      he;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type    zeroGradient;
        value   $internalField;
    }
}

